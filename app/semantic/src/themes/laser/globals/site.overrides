// x-editable & poshytip
//
.tip-yellowsimple {
    /*
    margin-top: 28px;
    margin-left: 58px;
    */
    margin-top: -1rem;
    z-index: 10;
    text-align: left;
    position: absolute;
    text-transform: none;
    text-align: left;
    white-space: nowrap;
    font-size: 1rem;
    border: 1px solid #d4d4d5;
    line-height: 1.4285em;
    max-width: none;
    background: #fff;
    padding: .5em 0.5em;
    -webkit-box-shadow: 0 2px 4px 0 rgba(34, 36, 38, .12), 0 2px 10px 0 rgba(34, 36, 38, .15);
    box-shadow: 0 2px 4px 0 rgba(34, 36, 38, .12), 0 2px 10px 0 rgba(34, 36, 38, .15);
    form.form-inline > * {
        margin: 0 ;
    }
}

.tip-yellowsimple:before {
    box-sizing: inherit;
    box-shadow: 1px 1px 0 0 #bababc;
    z-index: 2;
    position: absolute;
    content: '';
    font-size: 1rem;
    width: .71428571em;
    height: .71428571em;
    transition: all .1s ease;
    transform-origin: center top;
    top: auto;
    right: auto;
    bottom: -14%;
    left: 50%;
    background: #fff;
    margin-left: -.07142857rem;
    margin-bottom: .14285714rem;
    visibility: visible;
    pointer-events: auto;
    transform: rotate(45deg) scale(1)!important;
    opacity: 1;
}

//  Overwrites x-editable
.editableform .control-group{
    background-color: #f4f8f9;
    padding: 10px;
    box-shadow: 1px 1px 2px 1px rgba(20,20,20,.3);
    position: relative;
    //left: -20px;
}
.editableform .control-group {
    select, input {
        width: auto!important;
    }
}
.editable-clear-x {
    background: none;
    height: 1em;
    color: @grey;
    display: block;
    margin: -9px 3px 0 0;
    font-size: 1.5em;
    transition: color 0.6s ease, transform 0.2s ease;
    font-family: Icons;
    font-style: normal;
    font-weight: 400;
    text-decoration: inherit;
    text-align: center;
    backface-visibility: hidden;
    z-index: 80;
    &:before {
        content: "\f2ed";
    }
}
.ui.table .editable-container.editable-inline {
    position: absolute;
    z-index: 100;
    margin-top: -29px;
    margin-left: -3px;
}
.editable-container.editable-inline {
    position: absolute;
    z-index: 100;
    margin-top: -20px;
    margin-left: -3px;
}
